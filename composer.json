{
    "name": "spiffy/spiffy-connect",
    "description": "SpiffyConnect is a connector implementing OAuth1 and OAuth2 to make communicating with service providers as easy as possible.",
    "homepage": "http://www.github.com/spiffyjr/spiffy-connect.git",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-crypt": "2.*",
        "zendframework/zend-http": "2.*",
        "zendframework/zend-json": "2.*"
    },
    "require-dev": {
        "mockery/mockery": "0.*",
        "phpunit/phpunit": "3.*",
        "spiffy/spiffy-test": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "SpiffyConnect": "src/",
            "SpiffyConnectTest": "test/"
        }
    }
}
